<?xml version="1.0"?>
<config>
    <modules>
        <SomethingDigital_InfluxDb>
            <version>0.5.0</version>
        </SomethingDigital_InfluxDb>
    </modules>
    <global>
        <models>
            <sd_influxdb>
                <class>SomethingDigital_InfluxDb_Model</class>
            </sd_influxdb>
        </models>
    </global>
    <frontend>
        <events>
            <controller_front_send_response_before>
                <observers>
                    <sd_influxdb>
                        <class>sd_influxdb/observer</class>
                        <method>handleControllerFrontSendResponseBefore</method>
                    </sd_influxdb>
                </observers>
            </controller_front_send_response_before>
        </events>
    </frontend>
    <default>
        <sd_influxdb>
            <general>
                <password backend_model="adminhtml/system_config_backend_encrypted" />
            </general>
            <fpc_hit_rate>
                <in_progress_expires>3600</in_progress_expires>
            </fpc_hit_rate>
        </sd_influxdb>
    </default>
</config>
